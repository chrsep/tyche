generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model tyche {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Insurance {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  content String
}

